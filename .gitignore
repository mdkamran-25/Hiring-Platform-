# Ignore Node.js dependencies
node_modules/
package-lock.json
pnpm-lock.yaml
yarn.lock

# Ignore logs and debug files
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Ignore environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore Next.js build output (Frontend)
frontend/.next/
frontend/out/
frontend/dist/

# Ignore TypeScript build info
frontend/*.tsbuildinfo
backend/*.tsbuildinfo

# Ignore Backend build output
backend/dist/
backend/.cache/

# Ignore Python cache files (AI directory)
ai/__pycache__/
ai/*.pyc
ai/*.pyo
ai/*.pyd

# Ignore model files (if applicable)
ai/models/

# Ignore database dumps or local DB files
backend/db.sqlite3
backend/*.db

# Ignore Firebase & Deployment artifacts
firebase-debug.log
firestore.indexes.json
firestore.rules
.deployment/
.deployment-config.json
deployment/.firebase/
deployment/*.log

# Ignore Docker files (if using Docker)
docker-compose.override.yml
*.dockerfile

# Ignore system & editor-specific files
.vscode/
.idea/
*.swp
.DS_Store
Thumbs.db

# Ignore Coverage & Test Reports
coverage/
test-results/
cypress/videos/
cypress/screenshots/
